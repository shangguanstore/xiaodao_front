button[class="no_border_radius"]::after {
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  border-radius: 0;
}

button[class="no_border"]::after {
  border: 0;
}

button.no_border_radius {
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  border-radius: 0;
}

button.no_border {
  border: 0;
}
