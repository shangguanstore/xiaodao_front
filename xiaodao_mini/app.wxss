/**app.wxss**/
@import 'weui.wxss';
@import 'weui-custom.wxss';
@import 'colors.wxss';
@import 'button.wxss';
/*@import 'xiaonengxiao.wxss';*/
@import 'measure.wxss';
@import 'xiaonengxiao_reference.wxss';

page {
  background: #f8f8f9;
  font-family: '微软雅黑';
  font-style: normal;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}

.mt10 {
  margin-top: 10rpx;
}
.mt20 {
  margin-top: 20rpx;
}

.ml10 {
  margin-left: 10rpx;
}
.ml20 {
  margin-left: 20rpx;
}
.mr10 {
  margin-right: 10rpx;
}
.mr20 {
  margin-right: 20rpx;
}



.rich-text {
  padding: 0 20rpx;
}

.hideWxBtn::after {
  border: 0;
}

.floatWrap::after {
  content: '';
  display: block;
  clear: both;
}
.fl {
  float: left;
}
.fr {
  float: right;
}

.mainBackgroundColor {
  background: rgba(0, 156, 240, 1);
}

.mainColor {
  color: rgba(0, 156, 240, 1);
}

.gap {
  width: 750rpx;
  height: 20rpx;
  background: #f1f1f1;
}

/* 加载中样式 */
.van-loading {
  position: fixed;
}
.loading {
  /* margin-right: 20px; */
  /* margin-top: 300rpx; */

  position: fixed;
  top:50%;
  left:50%;
  transform:translate(-50%, -50%);
}
.loadingShadow {
  padding: 10px;
  border-radius: 3px;
  background-color: rgba(0, 0, 0, 0.5);
}
/* 加载中样式结束 */

/* 底部提交按钮开始 */

.bottomBtn {
  width: 750rpx;
  height: 120rpx;
  background: white;
  position: fixed;
  bottom: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}

/* button.default {
	background-color: #ffffff;
	border-radius: 8rpx;
	border: solid 1rpx #6b67be;
  color: #6b67be;
} */

