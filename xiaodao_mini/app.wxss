	background-color: #6b67be;
	border-radius: 8rpx;
  color: white;
}

button[class="primary"]::after {
	border: 0;
} */

/* 底部提交按钮结束 */

/* field开始 */

.fieldArea {
  width: 720rpx;
  background: white;
  padding-left: 30rpx;
  border-top: solid 1rpx #e5e5e5;
  border-bottom: solid 1rpx #e5e5e5;
}

/* field结束 */

/* 订单商品块区域开始 */

.orderGoodsBlock {
  width: 705rpx;
  border-top: solid 1px #dcdee2;
  padding: 30rpx;
  overflow: hidden;
}

.orderGoodsBlock .imgArea {
  float: left;
  width: 147rpx;
  height: 100rpx;
}

.orderGoodsBlock .imgArea image {
  width: 100%;
  height: 100%;
}

.orderGoodsBlock .goodsContent {
  width: calc(90% - 147rpx);
  float: left;
  margin-left: 30rpx;
}

.orderGoodsBlock .goodsContent .title {
  color: #17233d;
  font-size: 36rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}

.orderGoodsBlock .goodsContent .description {
  font-size: 27rpx;
  color: #83878a;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}

.orderGoodsBlock .goodsContent .price {
  color: #808695;
  font-size: 28rpx;
}

.orderGoodsBlock .goodsContent .price .realPrice {
  color: #fa2c87;
  font-size: 34rpx;
  font-weight: 500;
}

.orderGoodsBlock .goodsContent .price .fullPrice {
  text-decoration: line-through;
  margin-left: 20rpx;
}

/* 订单商品块区域结束 */

.scroll-view_H {
  background: #31a89e;
  white-space: nowrap;
}

.scroll-view-item_H {
  float: left;
  margin: 20rpx -20rpx;
  margin-left: 40rpx;
  padding: 10rpx;
  padding-bottom: 3rpx;
  width: 652rpx;
}

/* 底部tab切换结束 */

.fixBottom {
  width: 100%;
  height: 98rpx;
  position: fixed;
  bottom: 0;
  display: flex;
  justify-content: center;
  background: white;
}

.fixBottom .item {
  width: 33.3%;
  float: left;
  text-align: center;
}

.fixBottom view image {
  width: 49rpx;
  height: 49rpx;
  margin-top: 15rpx;
}

.fixBottom view .title {
  margin-top: -15rpx;
  font-size: 20rpx;
  color: #aaa;
}

/* 活动列表 */

.listArea {
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}

.listArea .listItem {
  width: 702rpx;
  margin-top: 20rpx;
  background-color: #fff;
  box-shadow: 0rpx 6rpx 24rpx 0rpx rgba(83, 93, 99, 0.05);
  border-radius: 10rpx;
}

.listArea .listItem:first-child {
  margin-top: 0;
}

.listArea .listItem .imgBackground {
  position: relative;
  width: 100%;
  height: 460rpx;
  margin-top: 0;
  background-size: cover;
}

.listArea .listItem .img {
  width: 100%;
}

.listArea .listItem .img image {
  height: 346rpx;
  width: 100%;
}

.listArea .listItem .detail {
  margin: 20rpx 16rpx;
  padding-bottom: 20rpx;
  font-size: 30rpx;
  color: #444;
}

.listArea .listItem .bottom {
  width: 100%;
  display: flex;
  justify-content: space-between;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
  padding-bottom: 20rpx;
}

.listArea .listItem .bottom .titleArea {
  width: 500rpx;
}

.listArea .listItem .bottom .titleArea .title {
  font-size: 34rpx;
  color: #444;
  font-weight: 500;
  margin-bottom: 10rpx;
}

.listArea .listItem .bottom .titleArea .des {
  font-size: 26rpx;
  color: #666;
}

.listArea .listItem .bottom .btn {
  color: white;
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
}

.listArea .listItem .bottom .btn button {
  color: white;
  box-sizing: border-box;
  /*padding: 6rpx 20rpx;*/
  line-height: 56rpx;
  font-weight: 300;
  font-size: 32rpx;
  min-width: 186rpx;

}

/* 活动列表结束 */

/* 图片居中大小自适应 */

.centerPicBox {
  width: 100%;
  height: 346rpx;
  position: relative;
  overflow: hidden;
}

.centerPic {
  height: 346rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

/* 图片自适应居中结束 */

/*自定义列表开始*/

.my-list-container {
  width: 100%;
  background: white;
}

.my-list-container .list {
  border-bottom: solid 1rpx #e5e5e5;
  margin-left: 30rpx;
  padding: 30rpx 0;
  display: flex;
  justify-content: space-between;
}

.my-list-container .list:last-child {
  border: none;
}

.my-list-container .list text:first-child {
  font-size: 34rpx;
  color: #777;
}

.my-list-container .list text:last-child {
  padding-right: 30rpx;
  font-size: 34rpx;
  color: #444;
}

/*自定义列表结束*/

form {
  width: 70%;
  margin-top: 60rpx;
}

form input {
  border: solid 1rpx #dcdee2;
  height: 90rpx;
}

form text {
  font-size: 28rpx;
  color: #808695;
  margin-top: 10rpx;
}

/* 加载中 */

.icon-load {
  animation: myMove1 1s infinite;
  /* ease-in *//* infinite *//* alternate */
}

@keyframes myMove1 {
  from {
    transform: rotate(40deg);
  }

  to {
    transform: rotate(400deg);
  }
}

/* 加载中结束 */

/* 报名列表开始 */

.applyAvatarList {
  width: 100%;
  padding: 20rpx 0;
  display: flex;
  justify-content: flex-start;
}

.applyAvatarList .listItem {
  margin-right: 30rpx;
  text-align: center;
}

.applyAvatarList .listItem image {
  width: 70rpx;
  height: 70rpx;
  border-radius: 70rpx;
}

.applyAvatarList .listItem .name {
  margin-top: -10rpx;
  font-size: 24rpx;
  color: #666;
}

/* 报名列表结束 */

/*商城*/

.wxParse-img {
  /* display: block !important; */
}

.fixBottom {
  width: 100%;
  height: 98rpx;
  position: fixed;
  bottom: 0;
  display: flex;
  justify-content: center;
  background: white;
}

.fixBottom .item {
  width: 33.3%;
  float: left;
  text-align: center;
}

.fixBottom view image {
  width: 49rpx;
  height: 49rpx;
  margin-top: 15rpx;
}

.fixBottom view .title {
  margin-top: -15rpx;
  font-size: 20rpx;
  color: #aaa;
}

button[class="no_border_radius"]::after {
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  border-radius: 0;
}

button[class="no_border"]::after {
  border: 0;
}

button.no_border_radius {
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  border-radius: 0;
}

button.no_border {
  border: 0;
}

/*商城结束*/

.selectField {
  background: white;
  display: flex;
  justify-content: space-between;
  padding-left: 25rpx;
  align-items: center;
  color: #333;
  font-size: 33rpx;
}

/*授权弹出框开始*/

.popupBox {
  border-radius: 20rpx;
}

.weixinAuthBox {
  border-radius: 6rpx;
  padding: 20rpx 100rpx;
  /* width: 300rpx; */
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
}

.weixinAuthBox .title {
  width: 340rpx;
  height: 100rpx;
}

.egg {
  width: 300rpx;
  height: 324rpx;
}

.weixinAuthBox .btn {
  box-sizing: content-box;
  border-radius: 40rpx;
  padding: 8rpx 72rpx;
  color: white;
  font-size: 36rpx;
  margin: 20rpx 0;
  line-height: 74rpx;
}

/*授权弹出框结束*/


/* cell区域开始 */
.cellDetailArea {
  width: 100%;
  padding-left: 30rpx;
  background: white;
  border-top: solid 1rpx #e5e5e5;
  border-bottom: solid 1rpx #e5e5e5;
}

.cellDetailArea .detailItem {
  display: flex;
  justify-content: space-between;
  padding: 27rpx 0;
  padding-right: 30rpx;
  border-bottom: solid 1px #e5e5e5;
}

.cellDetailArea .picItem {
  padding: 27rpx 0;
  padding-right: 30rpx;
  border-bottom: solid 1px #e5e5e5;
}

.cellDetailArea .picItem .label {
  font-size: 34rpx;
  color: #777;
}

.cellDetailArea .picItem image {
  width: 140rpx;
  height: 140rpx;
  margin-top: 20rpx;
}

.cellDetailArea .detailItem:last-child {
  border: 0;
}

.cellDetailArea .detailItem .label {
  font-size: 34rpx;
  color: #777;
}

.cellDetailArea .detailItem .value {
  font-size: 34rpx;
  color: #444;
}
/* cell 区域结束 */

.ellipsis {
  display: inline-block;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}

/* icon - front-mobile 项目 */
@import 'iconfont.wxss';

/* icon - front-mobile 项目 结束 */
