  padding: 8rpx 32rpx;
  border-radius: 8rpx;
}

.table {
  min-height: 70rpx;
  padding: 0;
}

.table>text {
  text-align: center;
  min-height: 70rpx;
  line-height: 70rpx;
}

.table>button {
  background-color: white;
  height: 70rpx;
  line-height: 70rpx;
  color: #202020;
  font-size: 22rpx;
}

.ellihide {
  display: inline-block;
  overflow: hidden;
  white-space: nowrap;
}

.schedule-search {
  position: relative;
  height: 54rpx;
  border: 2rpx solid #d7d7d7;
  border-radius: 10rpx;
  padding-left: 16rpx;
}

.schedule-search .select {
  position: absolute;
  font-size: 26rpx;
  top: 8rpx;
  right: 16rpx;
}

.schedule-top {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 10;
}

.bg-img {
  position: absolute;
  top: 0;
  left: 0;
  width: 750rpx;
  height: 600rpx;
}

.register {
  position: fixed;
  right: 30rpx;
  bottom: 60rpx;
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
  background-color: rgba(255,91,43,0.4);
}

.register view {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  background: linear-gradient(to right,#FF3B3B,#FF8416);
}

.register text {
  font-size: 34rpx;
  color: white;
  line-height: 1.2;
}

.smallbtn {
  width: 100rpx!important;
  height: 100rpx!important;
}

.smallbtn view {
  width: 84rpx!important;
  height: 84rpx!important;
}

.smallbtn text {
  font-size: 24rpx!important;
}

.radius-6 {
  border-radius: 12rpx!important;
}

.radius-6 .picbook-item {
  border-radius: 12rpx!important;
}

.radius-6 image {
  border-top-left-radius: 12rpx;
  border-top-right-radius: 12rpx;
}

.h5link {
  color: blue;
  text-align: center;
  padding: 4rpx 30rpx;
  font-size: 22rpx;
}

.title-label {
  display: inline-block;
  width: 150rpx;
  text-align: right;
}

.title-content {
  display: inline-block;
  width: 460rpx;
  padding: 0 30rpx;
  min-height: 60rpx;
  line-height: 60rpx;
  background-color: #F1F1F1;
}

.rank-block {
  background-image: url(https://static.toumashi.com/static/collect/order_bg.png);
  background-size: 100% auto;
  background-repeat: no-repeat;
  padding: 120rpx 32rpx 32rpx 32rpx;
}

.rank-block .my-rank {
  border-radius: 10rpx;
  box-shadow: 0 0 20rpx rgba(112,112,112,0.27);
  width: 414rpx;
  height: 175rpx;
  position: relative;
  margin: 0 auto 20rpx auto;
}

.rank-block .my-rank .icon-round {
  position: absolute;
  top: 0;
  left: 50%;
  transform: translate(-50%,-50%);
}

.blue_box .mid_bg {
  background-color: #2b8aff;
  color: white;
}

.blue_box .lightest_bg {
  background-color: #d9eaff!important;
}

.blue_box .lightest_border {
  border: 2rpx solid #d9eaff;
}

.blue_box .mid_border {
  border: 2rpx solid #2b8aff;
}

.blue_box .lighter_bg {
  background-color: #85bcff;
}

.blue_box .mid_text {
  color: #2b8aff;
}

.blue_box .dark_text {
  color: #0068e8;
}

.blue_box .darkest_text {
  color: #163c6a;
}

.blue_box .ltitle::after {
  background-color: #2b8aff;
}

.barrage-area {
  position: relative;
  width: 750rpx;
  overflow: hidden;
}

.barrage-area .barrage-item {
  z-index: 20;
  position: absolute;
  left: 0;
  visibility: hidden;
  animation: barrage 15s linear;
}

.barrage-area .barrage-item image {
  position: absolute;
  left: -7rpx;
  top: -5rpx;
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
}

.barrage-area .barrage-item>view {
  height: 50rpx;
  padding: 0 25rpx 0 64rpx;
  border-radius: 25rpx;
  background: linear-gradient(0deg,rgba(230,32,0,0.57) 0%,rgba(249,76,0,0.57) 100%);
}

.barrage-area .barrage-item>view text {
  line-height: 50rpx;
}

.barrage-area .barrage-item:nth-child(3n+1) {
  top: 16rpx;
}

.barrage-area .barrage-item:nth-child(3n+2) {
  top: 90rpx;
}

.barrage-area .barrage-item:nth-child(3n) {
  top: 160rpx;
}

@keyframes barrage {
  from {
    visibility: visible;
    transform: translateX(750rpx);
  }

  to {
    visibility: visible;
    transform: translateX(-750rpx);
  }
}

.bgm-btn {
  position: absolute;
  top: 20rpx;
  left: 14rpx;
  width: 50rpx;
  height: 50rpx;
  border-radius: 25rpx;
  background-color: rgba(0,0,0,0.5);
  z-index: 3000;
}

.bgm-on {
  animation: music 3s linear infinite;
}

@keyframes music {
  from {
    transform: rotateZ(0);
  }

  to {
    transform: rotateZ(360deg);
  }
}