.chat-box {
  position: relative;
  background-color: #f1f1f1;
  border-radius: 8rpx;
  margin-bottom: 20rpx;
  padding: 20rpx 24rpx;
}

.chat-box .del {
  position: absolute;
  font-size: 44rpx;
  color: #909090;
  right: -60rpx;
  top: 50%;
  transform: translateY(-50%);
}

.chat-box .del.copy {
  right: -120rpx;
}

.chat-box::after {
  position: absolute;
  top: 20rpx;
  left: -36rpx;
  content: '';
  border: 24rpx solid transparent;
  border-right: 24rpx solid #f1f1f1;
}

.chat-box .voice>view {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 4rpx;
  width: 18rpx;
  height: 18rpx;
  border: 4rpx solid #909090;
  border-radius: 50%;
  clip-path: polygon(100% 100%,100% 0,50% 50%);
}

.chat-box .voice>view:first-child {
  width: 30rpx;
  height: 30rpx;
  left: 0;
}

.chat-box .voice>view:last-child {
  width: 44rpx;
  height: 44rpx;
  left: -4rpx;
}

.chat-box .active>view:first-child {
  animation: playvoices 1.6s linear infinite;
}

.chat-box .active>view:nth-child(2) {
  animation: playvoicem 1.6s linear infinite;
}

.chat-box .active>view:last-child {
  animation: playvoicel 1.6s linear infinite;
}

@keyframes playvoicem {
  0% {
    border: 4rpx solid transparent;
  }

  25% {
    border: 4rpx solid #909090;
  }

  50% {
    border: 4rpx solid #909090;
  }

  75% {
    border: 4rpx solid #909090;
  }

  100% {
    border: 4rpx solid transparent;
  }
}

@keyframes playvoices {
  0% {
    border: 4rpx solid transparent;
  }

  25% {
    border: 4rpx solid transparent;
  }

  50% {
    border: 4rpx solid #909090;
  }

  75% {
    border: 4rpx solid #909090;
  }

  100% {
    border: 4rpx solid transparent;
  }
}

@keyframes playvoicel {
  0% {
    border: 4rpx solid transparent;
  }

  25% {
    border: 4rpx solid transparent;
  }

  50% {
    border: 4rpx solid transparent;
  }

  75% {
    border: 4rpx solid #909090;
  }

  100% {
    border: 4rpx solid transparent;
  }
}

